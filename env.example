# Configuración de Base de Datos Supabase
# Copia este archivo como .env y reemplaza los valores con los de tu proyecto

# URL de conexión completa (reemplaza con tu connection string de Supabase)
DATABASE_URL=postgresql+asyncpg://postgres:[YOUR_PASSWORD]@db.[YOUR_PROJECT_REF].supabase.co:5432/postgres

# URL para Alembic (sin asyncpg)
ALEMBIC_DATABASE_URL=postgresql://postgres:[YOUR_PASSWORD]@db.[YOUR_PROJECT_REF].supabase.co:5432/postgres

# Configuración de JWT (opcional, para autenticación)
JWT_SECRET_KEY=your-super-secret-jwt-key-here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configuración de Redis (si usas cache)
REDIS_URL=redis://localhost:6379

# Configuración de la aplicación
ENVIRONMENT=development
DEBUG=True
